<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vakery.ics">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Presentation.MainActivity"
            android:screenOrientation="portrait">
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Presentation.SubjectInfoActivity" />
        <activity android:name=".Presentation.LecturerInfoActivity" />
        <activity android:name=".Presentation.LecturersActivity" />
        <activity android:name=".Presentation.SubjectsActivity" />
        <activity android:name=".Presentation.MarksActivity" />
        <activity android:name=".Presentation.ProgramsActivity" />
        <activity android:name=".EntropyCalculator"></activity>
    </application>

</manifest>